{
	"name": "app-home",
	"version": "2.0.0",
	"private": false,
	"scripts": {
		"build": "rm -rf ./build && babel ./src --out-dir=build --source-maps --copy-files --quiet",
		"debug": "npm run transpile && nodemon --exec babel-node -- src/boot/debug.js",
		"start": "node ./build/boot/start.js",
		"transpile": "gulp"
	},
	"dependencies": {
		"babel-polyfill": "^6.23.0",
		"body-parser": "^1.17.1",
		"cuvva-log-sentry": "^0.5.2",
		"cuvva-log": "^0.9.1",
		"express-handlebars": "^3.0.0",
		"express-promise-router": "^1.1.1",
		"express": "^4.15.2",
		"handlebars": "^4.0.8",
		"json-client": "^0.8.4",
		"moment-timezone": "^0.5.13",
		"query-string": "^4.3.4",
		"raven": "^2.0.0"
	},
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-plugin-syntax-flow": "^6.18.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-flow-strip-types": "^6.22.0",
		"babel-plugin-typecheck": "^3.9.0",
		"babel-preset-env": "^1.4.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"gulp": "^3.9.1",
		"gulp-sass": "^3.1.0",
		"nodemon": "^1.11.0"
	},
	"babel": {
		"presets": [
			"es2015",
			"stage-2"
		],
		"plugins": [
			"typecheck",
			"syntax-flow",
			"transform-flow-strip-types",
			"transform-class-properties",
			"eslint-plugin-import"
		]
	}
}
