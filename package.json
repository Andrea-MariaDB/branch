  },
  "license": "DBAD",
  "scripts": {
    "build": "rm -rf ./build && babel ./src --out-dir=build --source-maps --copy-files --quiet",
    "debug": "npm run transpile && nodemon --exec babel-node -- src/boot/debug.js",
    "start": "node ./build/boot/start.js",
    "transpile": "gulp"
  },
  "dependencies": {
    "@branch-app/log": "^0.9.1",
    "@branch-app/log-sentry": "^0.5.2",
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.2",
    "bootstrap": "3.3.7",
    "express": "^4.15.3",
    "express-handlebars": "^3.0.0",
    "express-hbs": "^1.0.4",
    "express-promise-router": "^1.1.1",
    "humanize-duration": "^3.10.0",
    "jquery": "3.2.1",
    "json-client": "^0.8.4",
    "moment-timezone": "^0.5.13",
    "number_helpers": "^0.1.1",
    "pluralize": "^5.0.0",
    "query-string": "^4.3.4",
    "raven": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^7.3.0",
    "bower": "^1.8.0",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "eslint": "^4.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.6.0",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-browserify": "^0.5.1",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "merge-stream": "^1.0.1",
    "nodemon": "^1.11.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
    "typed.js": "./bower_components/typed.js/lib/typed.min.js"
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "typed.js": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "typecheck",
      "syntax-flow",
      "transform-flow-strip-types",
      "transform-class-properties"
    ]
  }
}
